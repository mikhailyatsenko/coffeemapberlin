{
  "redirects": [
    {
      "source": "/details",
      "has": [
        {
          "type": "query",
          "key": "id",
          "value": "66(.*)"
        }
      ],
      "destination": "/404",
      "statusCode": 404
    }
  ],
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    {
      "source": "/details",
      "has": [
        {
          "type": "query",
          "key": "id"
        }
      ],
      "destination": "/api/redirect-details"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true,
      "**": false
    }
  }
}