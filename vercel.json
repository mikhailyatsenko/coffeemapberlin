{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/details",
      "has": [
        {
          "type": "query",
          "key": "id",
          "value": "(?<id>.*)"
        }
      ],
      "destination": "/$id",
      "permanent": true
    },
    {
      "source": "/details",
      "has": [
        {
          "type": "query",
          "key": "id",
          "value": "(?<id>.*)"
        }
      ],
      "destination": "/$id",
      "permanent": true
    }
  ],

  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true,
      "**": false
    }
  }
}